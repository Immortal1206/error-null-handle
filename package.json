{
  "name": "error-null-handle",
  "version": "0.1.7",
  "description": "function programming approach for error and null handling",
  "main": "./dist/lib/index.js",
  "module": "./dist/es/index.js",
  "types": "./dist/es/index.d.ts",
  "type": "module",
  "scripts": {
    "build": "tsc -p ./tsconfig.es.json && tsc -p ./tsconfig.cjs.json && rollup -c rollup.config.js",
    "rollup:build": "rollup -c rollup.config.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Immortal1206/error-null-handle"
  },
  "exports": {
    ".": {
      "require": "./dist/lib/index.js",
      "import": "./dist/es/index.js"
    }
  },
  "keywords": [
    "typescript",
    "error handle",
    "null handle"
  ],
  "author": "lucifer",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-terser": "^0.4.4",
    "rollup": "^4.5.0",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-typescript2": "^0.36.0",
    "typescript": "^5.2.2"
  },
  "files": [
    "dist/",
    "LICENSE",
    "README.md",
    "src"
  ]
}
